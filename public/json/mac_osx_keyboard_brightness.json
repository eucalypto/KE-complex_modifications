{
  "title": "Keyboard brightness/illumination control",
  "maintainers": [
    "mynameisnextstep"
  ],
  "rules": [
    {
      "description": "Keyboard brightness/illumination control",
      "manipulators": [
        {
          "from": {
            "key_code": "f1",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "halt": false,
              "key_code": "illumination_decrement",
              "lazy": true,
              "repeat": true
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "f2",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "halt": false,
              "key_code": "illumination_increment",
              "lazy": true,
              "repeat": true
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}