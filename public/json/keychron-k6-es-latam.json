              "key_code": "e",
              "modifiers": ["option"]
            },
            {
              "key_code": "e",
              "modifiers": ["shift"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "modifiers": { "optional": ["any"] },
